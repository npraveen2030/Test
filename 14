                    AnalysisId = 100823, BillingMethod = "Quarterly", Description = "Test 5", SetupFree = true
                }
            };

            // Lambda to get list of 5 where AnalysisId == 100823
            var result = data.Where(x => x.AnalysisId == 100823).Take(5).ToList();

            // Output for test
            foreach (var item in result)
            {
                Console.WriteLine($"Element: {item.Element}, AnalysisId: {item.AnalysisId}, Price: {item.StandardPrice}");
            }
