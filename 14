                    ElementID = 2, ChargeClass = 'B', TieredType = 'C', Tier = 2, KeyAccount = "Key2",
                    AnalysisId = 100823, BillingMethod = "Quarterly", Description = "Test 2", SetupFree = false
                },
                new AdjustedElementDataModel
                {
                    Element = "Element3", PRName = "PR3", LongName = "Long Name 3", AfpCode = "A3",
                    StandardPrice = 20.00m, Tier1Volume = 130, Tier2Price = 18.5m, Tier2Volume = 170,
