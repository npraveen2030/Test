public class InterestExpensesData
{
    public int Id { get; set; }
    public int ModelId { get; set; }
    public int AccountType { get; set; }
    public decimal AvgColBal { get; set; }
    public decimal CurrentIntRate { get; set; }
    public decimal ExceptionIntRate { get; set; }
    public bool UseFedFunds { get; set; }
    public decimal FedFundsRate { get; set; }
    public string AccountNumber { get; set; }
    public decimal ExceptionIntRateBonus { get; set; }
    public decimal CurrentRate { get; set; }
    public string InterestType { get; set; }
    public DateTime RateExpiration { get; set; }
    public bool DerivedFromHogan { get; set; }
    public decimal FdicCost { get; set; }
    public decimal FdicRate { get; set; }
    public DateTime AppliedExpiration { get; set; }
    public bool Collateralized { get; set; }
    public int BonusType { get; set; }
    public int CampaignId { get; set; }
    public string ProductGroup { get; set; }
    public string EIEligibility { get; set; }
    public string EIEligibilityMsg { get; set; }
    public int MaxBalance { get; set; }
    public decimal MaxAppliedInterestRate { get; set; }
    public int Include { get; set; }
}
var interestExpensesList = new List<InterestExpensesData>
{
    new InterestExpensesData
    {
        Id = 1,
        ModelId = 1001,
        AccountType = 1,
        AvgColBal = 250000.00m,
        CurrentIntRate = 2.5m,
        ExceptionIntRate = 3.0m,
        UseFedFunds = true,
        FedFundsRate = 1.8m,
        AccountNumber = "ACC123",
        ExceptionIntRateBonus = 0.25m,
        CurrentRate = 2.75m,
        InterestType = "Variable",
        RateExpiration = DateTime.Now.AddMonths(6),
        DerivedFromHogan = false,
        FdicCost = 0.12m,
        FdicRate = 0.1m,
        AppliedExpiration = DateTime.Now.AddYears(1),
        Collateralized = true,
        BonusType = 1,
        CampaignId = 5001,
        ProductGroup = "Group A",
        EIEligibility = "Eligible",
        EIEligibilityMsg = "Meets criteria",
        MaxBalance = 1000000,
        MaxAppliedInterestRate = 3.25m,
        Include = 1
    },
    new InterestExpensesData
    {
        Id = 2,
        ModelId = 1002,
        AccountType = 2,
        AvgColBal = 150000.00m,
        CurrentIntRate = 1.75m,
        ExceptionIntRate = 2.0m,
        UseFedFunds = false,
        FedFundsRate = 0.0m,
        AccountNumber = "ACC456",
        ExceptionIntRateBonus = 0.15m,
        CurrentRate = 1.9m,
        InterestType = "Fixed",
        RateExpiration = DateTime.Now.AddMonths(3),
        DerivedFromHogan = true,
        FdicCost = 0.10m,
        FdicRate = 0.08m,
        AppliedExpiration = DateTime.Now.AddMonths(12),
        Collateralized = false,
        BonusType = 2,
        CampaignId = 5002,
        ProductGroup = "Group B",
        EIEligibility = "Not Eligible",
        EIEligibilityMsg = "Balance too low",
        MaxBalance = 500000,
        MaxAppliedInterestRate = 2.25m,
        Include = 0
    }
};

// ✅ Count all records
var totalRecords = interestExpensesList.Count();

// ✅ Optional: Count where Include == 1
var includedRecords = interestExpensesList.Count(x => x.Include == 1);

Console.WriteLine($"Total Records: {totalRecords}");
Console.WriteLine($"Included Records: {includedRecords}");
