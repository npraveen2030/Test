using (var context = new YourDbContext())
{
    // Fetch source records from AdjustedElementData based on SourceAnalysisID
    var sourceElements = context.AdjustedElementData
        .Where(a => a.AnalysisId == sourceAnalysisID)
        .Select(a => new AdjustedElementData
        {
            ModelId = modelID, // Assign the new Model ID
            AnalysisId = analysisID, // Assign the new Analysis ID
            Volume = a.Volume,
            UnitPrice = a.UnitPrice,
            ElementId = a.ElementId,
            CompPrice = a.CompPrice,
            ChargeClass = a.ChargeClass,
            StandardPrice = a.StandardPrice,
            Tier = a.Tier,
            CampaignId = a.CampaignId
        })
        .ToList(); // Retrieve all matching records

    if (sourceElements.Any()) // Ensure there are records to insert
    {
        context.AdjustedElementData.AddRange(sourceElements); // Insert all records
        context.SaveChanges(); // Save changes to the database

        Console.WriteLine($"Inserted {sourceElements.Count} records successfully.");
    }
}
