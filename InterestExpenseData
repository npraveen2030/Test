using (var context = new YourDbContext())
{
    // Fetch records matching the given ModelID
    var interestExpenseRecords = context.InterestExpenseData
        .Where(i => i.ModelId == modelID) // WHERE modelid = @ModelID
        .Select(i => new InterestExpenseData
        {
            ModelId = newModelID, // Assign the new model ID
            AccountType = i.AccountType,
            AvgColBal = i.AvgColBal,
            CurrentIntRate = i.CurrentIntRate,
            ExceptionIntRate = i.ExceptionIntRate,
            UseFedFunds = i.UseFedFunds,
            FedFundsRate = i.FedFundsRate,
            AccountNumber = i.AccountNumber,
            ExceptionIntRateBonus = i.ExceptionIntRateBonus,
            RateExpiration = i.RateExpiration,
            DerivedFromHogan = i.DerivedFromHogan,
            AppliedExpiration = i.AppliedExpiration,
            InterestType = i.InterestType,
            Collateralized = i.Collateralized
        })
        .ToList(); // Convert to a list for batch insert

    if (interestExpenseRecords.Any()) // Ensure we have data to insert
    {
        context.InterestExpenseData.AddRange(interestExpenseRecords); // Bulk insert
        context.SaveChanges(); // Save changes to the database
    }
}
