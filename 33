using System;
using System.Collections.Generic;
using System.Linq;

namespace DummyModelAnalysis
{
    public class ModelAnalysisData
    {
        public int ModelId { get; set; }
        public decimal TierMinBalance { get; set; }
        public string Message { get; set; }
        public decimal PositiveCollectedBalance { get; set; }
    }

    class Program
    {
        static void Main(string[] args)
        {
            // Generate 5 dummy records
            List<ModelAnalysisData> dataList = new List<ModelAnalysisData>
            {
                new ModelAnalysisData { ModelId = 100823, TierMinBalance = 1000.00m, Message = "Tier 1", PositiveCollectedBalance = 5000.00m },
                new ModelAnalysisData { ModelId = 100823, TierMinBalance = 2000.00m, Message = "Tier 2", PositiveCollectedBalance = 6000.00m },
                new ModelAnalysisData { ModelId = 100823, TierMinBalance = 3000.00m, Message = "Tier 3", PositiveCollectedBalance = 7000.00m },
                new ModelAnalysisData { ModelId = 100823, TierMinBalance = 4000.00m, Message = "Tier 4", PositiveCollectedBalance = 8000.00m },
                new ModelAnalysisData { ModelId = 999999,  TierMinBalance = 5000.00m, Message = "Other Tier", PositiveCollectedBalance = 9000.00m },
            };

            // Lambda to get rows where ModelId == 100823
            var filteredList = dataList.Where(x => x.ModelId == 100823).ToList();

            // Print results
            foreach (var item in filteredList)
            {
                Console.WriteLine($"ModelId: {item.ModelId}, TierMinBalance: {item.TierMinBalance}, Message: {item.Message}, PositiveCollectedBalance: {item.PositiveCollectedBalance}");
            }
        }
    }
}
