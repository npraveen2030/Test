public class ExceptionName
{
    public string Name { get; set; }
}

public List<ExceptionName> ExceptionNames = new()
{
    new ExceptionName { Name = "Praveen1" },
    new ExceptionName { Name = "Praveen2" },
    new ExceptionName { Name = "Praveen3" }
};


