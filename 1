using System;
using System.Collections.Generic;
using System.Linq;

namespace InterestExpenseApp
{
    public class InterestExpense
    {
        public int ModelId { get; set; }
        public int AccountType { get; set; }
        public decimal AvgColBal { get; set; }
        public decimal CurrentIntRate { get; set; }
        public decimal ExceptionIntRate { get; set; }
        public bool UseFedFunds { get; set; }
        public decimal FedFundsRate { get; set; }
        public decimal ExceptionIntRateBonus { get; set; }
        public decimal CurrentRate { get; set; }
        public string InterestType { get; set; }
        public DateTime RateExpiration { get; set; }
        public DateTime AppliedExpiration { get; set; }
        public bool Collateralized { get; set; }
        public decimal FdicCost { get; set; }
        public decimal FdicRate { get; set; }
    }

    class Program
    {
        static void Main(string[] args)
        {
            List<InterestExpense> expenses = new List<InterestExpense>
            {
                new InterestExpense
                {
                    ModelId = 100823,
                    AccountType = 1,
                    AvgColBal = 1000.00m,
                    CurrentIntRate = 0.5m,
                    ExceptionIntRate = 0.6m,
                    UseFedFunds = true,
                    FedFundsRate = 0.25m,
                    ExceptionIntRateBonus = 0.05m,
                    CurrentRate = 0.55m,
                    InterestType = "Fixed",
                    RateExpiration = DateTime.Now.AddMonths(1),
                    AppliedExpiration = DateTime.Now.AddMonths(2),
                    Collateralized = false,
                    FdicCost = 0.01m,
                    FdicRate = 0.02m
                },
                new InterestExpense
                {
                    ModelId = 100823,
                    AccountType = 2,
                    AvgColBal = 2000.00m,
                    CurrentIntRate = 0.6m,
                    ExceptionIntRate = 0.7m,
                    UseFedFunds = false,
                    FedFundsRate = 0.2m,
                    ExceptionIntRateBonus = 0.1m,
                    CurrentRate = 0.65m,
                    InterestType = "Variable",
                    RateExpiration = DateTime.Now.AddMonths(3),
                    AppliedExpiration = DateTime.Now.AddMonths(4),
                    Collateralized = true,
                    FdicCost = 0.015m,
                    FdicRate = 0.025m
                },
                new InterestExpense
                {
                    ModelId = 100823,
                    AccountType = 3,
                    AvgColBal = 1500.00m,
                    CurrentIntRate = 0.55m,
                    ExceptionIntRate = 0.65m,
                    UseFedFunds = true,
                    FedFundsRate = 0.23m,
                    ExceptionIntRateBonus = 0.08m,
                    CurrentRate = 0.6m,
                    InterestType = "Fixed",
                    RateExpiration = DateTime.Now.AddMonths(5),
                    AppliedExpiration = DateTime.Now.AddMonths(6),
                    Collateralized = false,
                    FdicCost = 0.02m,
                    FdicRate = 0.03m
                },
                new InterestExpense
                {
                    ModelId = 100823,
                    AccountType = 4,
                    AvgColBal = 2500.00m,
                    CurrentIntRate = 0.7m,
                    ExceptionIntRate = 0.8m,
                    UseFedFunds = false,
                    FedFundsRate = 0.22m,
                    ExceptionIntRateBonus = 0.09m,
                    CurrentRate = 0.75m,
                    InterestType = "Variable",
                    RateExpiration = DateTime.Now.AddMonths(7),
                    AppliedExpiration = DateTime.Now.AddMonths(8),
                    Collateralized = true,
                    FdicCost = 0.025m,
                    FdicRate = 0.035m
                },
                new InterestExpense
                {
                    ModelId = 100823,
                    AccountType = 5,
                    AvgColBal = 3000.00m,
                    CurrentIntRate = 0.65m,
                    ExceptionIntRate = 0.75m,
                    UseFedFunds = true,
                    FedFundsRate = 0.26m,
                    ExceptionIntRateBonus = 0.07m,
                    CurrentRate = 0.7m,
                    InterestType = "Fixed",
                    RateExpiration = DateTime.Now.AddMonths(9),
                    AppliedExpiration = DateTime.Now.AddMonths(10),
                    Collateralized = false,
                    FdicCost = 0.03m,
                    FdicRate = 0.04m
                }
            };

            // Lambda expression to filter by ModelId and take 5 rows
            var filteredList = expenses
                .Where(x => x.ModelId == 100823)
                .Take(5)
                .ToList();

            // Output to verify
            foreach (var item in filteredList)
            {
                Console.WriteLine($"AccountType: {item.AccountType}, CurrentRate: {item.CurrentRate}, InterestType: {item.InterestType}");
            }
        }
    }
}
